{
  "expo": {
    "name": "Taskusalkku",
    "slug": "taskusalkku",
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./index.tsx",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app optionally uses the camera (Face ID) to authenticate users."
      },
      "bundleIdentifier": "fi.metatavu.seligson.taskusalkku.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "fi.metatavu.seligson.taskusalkku.app"
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "privacy": "unlisted",
    "scheme": "fi.metatavu.seligson.taskusalkku"
  }
}
